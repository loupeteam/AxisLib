{
  "name": "@loupeteam/piperpkg",
  "version": "0.1.8",
  "description": "PiperPkg includes the Piper library as well as vars and types, tasks, templates, and CPU deployments. This makes it easy to get started with Piper on a new machine.",
  "scripts": {},
  "keywords": [],
  "author": "Loupe",
  "license": "Loupe Software License Agreement",
  "repository": {
    "type": "git",
    "url": "https://github.com/loupeteam/Piper/"
  },
  "dependencies": {
    "@loupeteam/piper": "0.1.8",
    "@loupeteam/hmitools": "*"
  },
  "lpm": {
    "type": "package",
    "logical": {
      "destination": "MachineControl"
    },
    "physical": {
      "cpu": [
        {
          "source": "MachineProg",
          "destination": "TC4"
        },
        {
          "source": "MachineHMI",
          "destination": "TC8"
        }
      ]
    }
  }
}
