{
  "name": "@loupeteam/acp10motionprog",
  "version": "0.1.0",
  "description": "This is a program package that will add the ACP10 Servo Manager program to your AS project",
  "author": "Loupe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loupeteam/AxisLib.git"
  },
  "dependencies": {
    "@loupeteam/axiserrlib": "^0.0.6",
    "@loupeteam/errorprog": "^0.1.0",
    "@loupeteam/firstinitprog": "^0.1.1",
    "@loupeteam/piperprog": "^0.1.0",
    "@loupeteam/axislib": "3.7.0"
  },
  "lpm": {
    "type": "program",
    "logical": {
      "destination": "Servo"
    },
    "physical": {
      "cpu": [
        {
          "source": "ServoMgr",
          "destination": "TC3"
        }
      ]
    }
  }
}
